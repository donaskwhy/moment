<h1>게시물 수정 페이지</h1>

<%= form_for(Board.edit, url: "edit_board_path", method: 'post') do |f| %>
    <%= f.label :title, "글 제목" %>
    <%= f.text_field :title %><br>
    
    <%= f.lebel :user_name, "작성자 이름" %>
    <%= f.text_field :user_name %><br>
    
    <%= f.lebel :content, "글 내용", rows: "3" %>
    <%= f.text_area :content %><br>
    
    <%= f.submit "제출" %>
    
<% end %>